        ;HTML and React tools
        hiccup/hiccup                                   {:mvn/version "1.0.5"}
        reagent/reagent                                 {:mvn/version "1.1.1"}
        reagent-utils/reagent-utils                     {:mvn/version "0.3.4"}
        pez/clerk                                       {:mvn/version "1.0.0"}

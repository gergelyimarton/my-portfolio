 :aliases
 {:dev  {:exec-fn    genesis.backend/dev
         :exec-args  {:shadow-build :app
                      :port         3000}
         :extra-deps {binaryage/devtools               {:mvn/version "1.0.0"}
                      thheller/shadow-cljs             {:mvn/version "2.18.0"}
                      com.wsscode/pathom-viz-connector {:mvn/version "2021.01.25"}
                      org.clojure/tools.namespace      {:mvn/version "1.0.0"}}}
  :prod {:extra-deps {com.github.seancorfield/depstar {:mvn/version "2.1.303"}
                      thheller/shadow-cljs             {:mvn/version "2.18.0"}}
         :exec-fn    genesis.compiler/compile-app!
         :exec-args {:java-config {:aot true
                                   :jar "wizard.jar"
                                   :jar-type :uberjar
                                   :main-class genesis.backend}
                         :js-builds [:app-ready]}}}}
